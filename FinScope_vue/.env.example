# Base URL for your Django API backend
VITE_API_BASE_URL="http://localhost:8000"

# Base URL for the Vue frontend (if needed for redirects, etc.)
VITE_LOCAL_BASE_URL="http://localhost:5173"

# Financial Supervisory Service (FSS) API Key (if used by frontend)
VITE_FSS_API_KEY="your_fss_api_key_here"

# Kakao Mobility REST API Key (for map services, etc.)
VITE_KAKAO_MOBILITY_REST_API_KEY="your_kakao_mobility_api_key_here"

# Google Login Client ID (for Google Sign-In)
VITE_GOOGLE_LOGIN_ID="your_google_client_id.apps.googleusercontent.com"

# Google Login Client Secret (IMPORTANT: Client secrets should ideally not be exposed in frontend code.)
# If your OAuth flow requires the secret on the client-side, be aware of security implications.
# Often, the secret is only used on the backend during token exchange.
# VITE_GOOGLE_LOGIN_SECRET="your_google_client_secret_here" # Commented out by default for security awareness

# Kakao Login Client Secret (JavaScript SDK Key or REST API Key might be used here depending on flow)
# Similar to Google, if this is a true "secret", it's better handled by backend.
# The Kakao JavaScript SDK uses the JavaScript App Key which is safe to expose.
# VITE_KAKAO_LOGIN_SECRET="your_kakao_login_secret_or_js_key" # Commented out by default

# Naver Login Callback URL (Must match the one registered with Naver)
VITE_NAVER_CALLBACK_URL="http://localhost:5173/login/naver/callback"

# YouTube API Key (for fetching video data)
VITE_YOUTUBE_API_KEY="your_youtube_api_key_here"
