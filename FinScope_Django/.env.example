# Django Secret Key (Keep this secret in production!)
SECRET_KEY="your_django_secret_key_here"

# OpenAI API Key (If used, otherwise remove)
OPENAI_API_KEY="sk-your_openai_api_key_here"

# Naver API Credentials (for search, etc.)
# Note: NAVER_CLIENT_ID and NAVER_CLIENT_SECRET might be for login, NAVER_API_ID/SECRET for other Naver services.
# Clarify if these are distinct or redundant.
NAVER_API_ID="your_naver_api_id"
NAVER_API_SECRET="your_naver_api_secret"

# Naver Login Client Credentials (if different from above)
NAVER_CLIENT_ID="your_naver_login_client_id"
NAVER_CLIENT_SECRET="your_naver_login_client_secret"

# Kakao REST API Key (for Kakao Login)
KAKAO_REST_API_KEY="your_kakao_rest_api_key"

# Korea Investment & Securities (KIS) API Credentials
KIS_API_ID="your_kis_api_id"
KIS_API_SECRET="your_kis_api_secret"

# Google Gemini API Key
GEMINI_API_KEY="your_gemini_api_key_here" # Corrected from GEMMINI

# Email Configuration (for sending emails, e.g., password reset)
EMAIL_HOST_USER="your_email_address@example.com"
EMAIL_HOST_PASSWORD="your_email_app_password"
# EMAIL_HOST="smtp.gmail.com" # Example for Gmail
# EMAIL_PORT=587
# EMAIL_USE_TLS=True

# Financial Supervisory Service (FSS) API Key
# Note: Variables starting with VITE_ are typically for Vue.js (.env in Vue project root)
# If this is used by Django backend, the VITE_ prefix is not standard.
# Consider renaming if it's purely a backend key, or moving to Vue .env if frontend.
VITE_FSS_API_KEY="your_fss_api_key"

# Database URL (Optional, Django typically uses settings.py for DB config)
# DATABASE_URL="sqlite:///./db.sqlite3" # Example for SQLite
# DATABASE_URL="postgres://user:password@host:port/dbname" # Example for PostgreSQL

# Debug Mode (True for development, False for production)
# DJANGO_DEBUG=True
